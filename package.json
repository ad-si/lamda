{
  "name": "lamda",
  "description": "A simple and modern web desktop environment",
  "version": "0.1.0",
  "bin": {
    "lamda": "bin/cli.js"
  },
  "scripts": {
    "start": "echo 'Use the lamda command line tool to start the server' && exit 1",
    "start-prod": "NODE_ENV=production node server.js",
    "start-dev": "NODE_ENV=development node server.js",
    "postinstall": "symlink-modules lamda-styles lamda-views",
    "lint": "eslint . ",
    "test": "npm run lint"
  },
  "homepage": "https://github.com/adius/lamda",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/adius/lamda"
  },
  "keywords": [
    "web os",
    "webtop",
    "web desktop",
    "web desktop environment"
  ],
  "author": "Adrian Sieber",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@datatypes/config": "^0.4.0",
    "commander": "^2.9.0",
    "compression": "^1.6.2",
    "deepmerge": "^1.3.2",
    "errorhandler": "^1.5.0",
    "express": "^4.14.1",
    "js-yaml": "^3.8.1",
    "lamda-styles": "git+ssh://git@github.com/adius/lamda-styles",
    "lamda-views": "git+ssh://git@github.com/adius/lamda-views",
    "morgan": "^1.8.1",
    "nib": "^1.1.2",
    "osenv": "^0.1.4",
    "pug": "^2.0.0-beta9",
    "serve-favicon": "^2.4.0",
    "stylus": "^0.54.5",
    "symlink-modules": "^0.2.0",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "eslint": "^3.16.1",
    "eslint-config-javascript": "^1.3.0"
  },
  "optionalDependencies": {
    "boilerplate": "git+ssh://git@github.com/adius/boilerplate",
    "books": "git+ssh://git@github.com/adius/books",
    "contacts": "git+ssh://git@github.com/adius/contacts",
    "events": "git+ssh://git@github.com/adius/events",
    "files": "git+ssh://git@github.com/adius/files",
    "movies": "git+ssh://git@github.com/adius/movies",
    "news": "git+ssh://git@github.com/adius/news",
    "photos": "git+ssh://git@github.com/adius/photos",
    "sheetmusic": "git+ssh://git@github.com/adius/sheetmusic",
    "songs": "git+ssh://git@github.com/adius/songs",
    "tasks": "git+ssh://git@github.com/adius/tasks",
    "things": "git+ssh://git@github.com/adius/things"
  },
  "eslintConfig": {
    "extends": "eslint-config-javascript"
  }
}
