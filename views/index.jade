extends ../../../views/layout

block styles
	link(rel='stylesheet', href='/assets/photos/public/index.css')

block content
	each year in years
		div
			h2= year.year
			hr
			if year.events
				each event in year.events
					div.event
						p: a(href=event.url)= event.name
						p.small= event.date
		br
		br
