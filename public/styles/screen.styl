#Projects
	padding 2em
	> div
		vertical-align top
		display inline-block
		background-color bgQuiet
		padding 1em
		margin 0 1em 1em 0
		min-width 120px

	.faviconCell
		text-align center
		padding 0

	.projectFavicon
		display block
		height 20px
		width 20px
		margin 0 auto
		image-rendering optimizeSpeed
		image-rendering -moz-crisp-edges
		image-rendering -o-crisp-edges
		image-rendering -webkit-optimize-contrast
		image-rendering optimize-contrast
		-ms-interpolation-mode nearest-neighbor
		&[src='']
			display none
	table
		td
			padding 0.4em 0.8em
